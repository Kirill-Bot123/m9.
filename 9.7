def is_prime(func):
    def wrapper(*nums: int):
        result_1 = func(*nums)
        if result_1 < 2:
            print('Число составное')
            return
        for i in range(2, result_1):
            if result_1 % i == 0:
                print('Число составное')
                break
        else:
            print('Число простое')
        return result_1
    return wrapper


@is_prime
def sum_three(first, second, third):
    return first + second + third


result = sum_three(2, 3, 6)
print(result)
